<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="6"
  SkinType="Component"
  Comment="HTML5 Toolbar skin"
  ComponentType="TopicToolbar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="ToolbarItem">
        <Classes>
          <StyleClass
            Name="SendFeedback"
            Custom="true">
            <Properties>
              <Property
                Name="BackgroundRepeat">no-repeat</Property>
              <Property
                Name="BackgroundPosition">center center</Property>
              <Property
                Name="Click">sendEmailToDocumentation()</Property>
              <Property
                Name="ButtonIcon">url('Message-Mail-22.png')</Property>
              <Property
                Name="ButtonTextDisplay">block</Property>
              <Property
                Name="BoxShadowVShadow">1px</Property>
              <Property
                Name="Width">170px</Property>
            </Properties>
          </StyleClass>
          <StyleClass
            Name="Share"
            Custom="true">
            <Properties>
              <Property
                Name="BackgroundRepeat">no-repeat</Property>
              <Property
                Name="BackgroundPosition">center center</Property>
              <Property
                Name="ButtonIcon">url('Share-06-WF-22.png')</Property>
              <Property
                Name="Click">sendEmail()</Property>
            </Properties>
          </StyleClass>
        </Classes>
        <Properties>
          <Property
            Name="BorderLeft">none 1px #e1e1e1</Property>
          <Property
            Name="BorderRight">none 1px #e1e1e1</Property>
          <Property
            Name="BorderTop">none 1px #e1e1e1</Property>
          <Property
            Name="BorderBottom">none 1px #e1e1e1</Property>
          <Property
            Name="BoxShadowVShadow">0px</Property>
          <Property
            Name="BackgroundColor">#ffffff</Property>
          <Property
            Name="BackgroundGradient">vertical #ffffff #ffffff</Property>
          <Property
            Name="BorderTopLeftRadius">0px</Property>
          <Property
            Name="BorderTopRightRadius">0px</Property>
          <Property
            Name="BorderBottomRightRadius">0px</Property>
          <Property
            Name="BorderBottomLeftRadius">0px</Property>
          <Property
            Name="FontFamily">Montserrat, sans-serif</Property>
          <Property
            Name="FontSize">16px</Property>
          <Property
            Name="Color">#006DB8</Property>
        </Properties>
      </Style>
      <Medium
        Name="Tablet">
        <Style
          Name="ToolbarItem">
          <Classes>
            <StyleClass
              Name="ShareFeedback">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
            <StyleClass
              Name="SendFeedback">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
            <StyleClass
              Name="Share">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
          </Classes>
        </Style>
      </Medium>
      <Medium
        Name="Mobile">
        <Style
          Name="ToolbarItem">
          <Classes>
            <StyleClass
              Name="ShareFeedback">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
            <StyleClass
              Name="SendFeedback">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
                <Property
                  Name="Width">22px</Property>
              </Properties>
            </StyleClass>
            <StyleClass
              Name="Share">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
          </Classes>
        </Style>
      </Medium>
    </Styles>
    <Stylesheet></Stylesheet>
    <Resources>
      <Resource
        Name="Share-06-WF-22.png"></Resource>
      <Resource
        Name="Message-Mail-22.png"></Resource>
    </Resources>
  </Stylesheet>
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="ToolbarItem">
            <Classes>
              <StyleClass
                Name="SendFeedback">
                <Properties>
                  <Property
                    Name="Label">Send feedback</Property>
                  <Property
                    Name="Tooltip">Send feedback</Property>
                  <Property
                    Name="AltText">Send feedback</Property>
                </Properties>
              </StyleClass>
              <StyleClass
                Name="Share">
                <Properties>
                  <Property
                    Name="Tooltip">Share</Property>
                  <Property
                    Name="AltText">Share</Property>
                  <Property
                    Name="Label">Share</Property>
                </Properties>
              </StyleClass>
            </Classes>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
  <TopicToolbar
    EnableCustomLayout="true"
    Buttons="Share|SendFeedback">
    <Script>function sendEmail() {
   const subject = "GigaSpaces Documentation - Topic Title: " + encodeURIComponent(document.title);
  const body = "I'm sharing this page with you:%0D%0A%0D%0A" + encodeURIComponent(window.location.href);
 
  const mailtoUrl = `mailto:?subject=${subject}&amp;body=${body}`;
  window.open(mailtoUrl);
}

function sendEmailToDocumentation() {
  const to = 'documentation@gigaspaces.com';
  const subject = "Topic Title: " + encodeURIComponent(document.title);
const body = "We%20appreciate%20your%20feedback.%20Please%20type%20your%20comments%20here...%0D%0A%0D%0A%0D%0A%0D%0A%0D%0A%0D%0A%0D%0A%0D%0A________________________________________________________________________________%0D%0ADocumentation%20team%20internal%20use%3A%0D%0AURL: " + encodeURIComponent(window.location.href);
   const mailtoUrl = `mailto:${to}?subject=${subject}&amp;body=${body}`;
  window.open(mailtoUrl);
}</Script>
  </TopicToolbar>
</CatapultSkin>